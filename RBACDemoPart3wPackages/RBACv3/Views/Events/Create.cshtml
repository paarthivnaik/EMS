
@{
    ViewBag.Title = "Create";
}


 <!-- /.row -->
<div class="row">
    <div class="col-md-12">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Create Event</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>

                </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="row">

                    <div class="col-lg-12">
                        <div class="row">


                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">*Customer Name</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-sort-alpha-asc input-group-addon"></i>
                                    <input type="text" id="AssetName" class="form-control" placeholder="Customer Name" name="AssetName" data-attr="AssetName" data-valt="required">

                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">*Mobile Number</strong>

                                <div class="input-group m-b">
                                    <i class="fa fa-list-alt input-group-addon"></i>
                                    <input type="text" placeholder="Mobile Number" class="form-control" data-attr="ServiceRequestID" id="ServiceRequestID" name="ServiceRequestID">
                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">Alternate Mobile</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-sort-numeric-asc input-group-addon"></i>
                                    <input type="text" placeholder="Alternate Mobile" class="form-control" data-attr="ServiceRequestID" id="ServiceRequestID" name="ServiceRequestID">
                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">Event Type</strong>

                                <div class="input-group m-b">
                                    <i class="fa fa-list-alt input-group-addon"></i>
                                    <select class="form-control DisposalReason" id="DisposalReason" name="DisposalReason" data-attr="DisposalReason" data-listentry="">
                                        <option value="">-- Select --</option>
                                        <option value="">Mehandi</option>
                                        <option value="">Sangeeth</option>
                                        <option value="">Birth Day</option>
                                    </select>
                                </div>
                            </div>



                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">Event Start Date</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-sitemap input-group-addon"></i>
                                    <input type="text" id="LostDestroyedStolenFrom" class="form-control" placeholder="Event Start Date" name="LostDestroyedStolenFrom" data-attr="LostDestroyedStolenFromName">

                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">Event End Date</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-sitemap input-group-addon"></i>
                                    <input type="text" id="LostDestroyedStolenFrom" class="form-control" placeholder="Event End Date" name="LostDestroyedStolenFrom" data-attr="LostDestroyedStolenFromName">

                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">* Venue</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Venue" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Venue" max="9999-12-31">
                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">* Package Name</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Package Name" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Package Name" max="9999-12-31">
                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">* Package Price</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Package Cost" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Package Price" max="9999-12-31">
                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">*Total Cost</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Total Cost" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Total Cost" max="9999-12-31">
                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">Advance</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Advance" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Advance" max="9999-12-31">
                                </div>

                            </div>

                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">*Event Manager Name</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Manager Name" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Event Manager Name" max="9999-12-31">
                                </div>

                            </div>
                            <div class="form-group col-lg-6">
                                <strong class="m-b-xs">*Mobile</strong>
                                <div class="input-group m-b">
                                    <i class="fa fa-calendar input-group-addon"></i>
                                    <input type="text" placeholder="Mobile" id="LostDestroyedStolenDate" class="form-control datepicker" data-attr="Mobile" max="9999-12-31">
                                </div>

                            </div>


                        </div>

                    </div>

                </div>
                <!-- /.row -->
            </div>
            <div class="box-footer clearfix">
                <a href="javascript:void(0)" class="btn btn-sm btn-info btn-flat pull-left">Save Event</a>
                <a href="javascript:void(0)" class="btn btn-sm btn-default btn-flat pull-right">Back</a>
            </div>
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->
</div>
<!-- /.row -->
 
















